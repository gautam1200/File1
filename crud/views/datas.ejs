<style>
    tr th{
        padding: 5px 10px;
        font-family: math;
        font-size: 24px;
    }
    tr td{
        padding: 5px 10px;
        font-family:  revert-layer;
        font-size: 20px;
    }
</style>


<form action="createData">
    <input type="text" name="name"> <br><br>
    <input type="password" name="password"> <br><br>
    <button type="submit">submit</button>
</form>


<table border="1">
    <tr >
        <th>Id</th>
        <th>Name</th>
        <th>password</th>
    </tr>
    <% data.map((item , index) => { %>
        <tr>
            <td ><%= index + 1 %></td>
            <td><%= item.name %></td>
            <td><%= item.password %></td>
        </tr>
   <% }) %>
</table>